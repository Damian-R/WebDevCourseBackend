<% include partials/header %>

<h1> Index Page </h1>

<% blogs.forEach(function(blog){ %>
    <div>
        <h2><%= blog.title %></h2>
        <img src="<%= blog.image %>">
        <p><%= blog.body %></p>
        <h4><%= blog.created %></h4>
    </div>
    <hr>
<% }) %>

<% include partials/footer %>