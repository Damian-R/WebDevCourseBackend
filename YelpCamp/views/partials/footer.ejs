    <% if(!currentUser) { %>
        <script>
            $(".auth-req").attr("data-target", ".loginmodal");
            $(".auth-req").attr("data-toggle", "modal");
            $(".auth-req").removeAttr("href", "");
            
            $(".auth-req").on("click", function(){
                $("#login-pill").addClass("active");
                $("#register-pill").removeClass("active");
                $("#mlogin").addClass("active in");
                $("#mregister").removeClass("active in");
            });
            
            $("#register-nav").on("click", function(){
                $("#register-pill").addClass("active");
                $("#login-pill").removeClass("active");
                $("#mregister").addClass("active in");
                $("#mlogin").removeClass("active in");
            });
            
        </script>
    <% } %>
